    "type": "wordpress-plugin",
    "license": "GPLv3",
    "authors": [
        {
            "name": "Ryan Hoover",
            "email": "ryanshoover@gmail.com"
        }
    ],
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
